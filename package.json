{
	"name": "dryvofront",
	"version": "0.0.6",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest"
	},
	"dependencies": {
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"@react-native-community/async-storage": "^1.4.1",
		"appcenter": "1.12.2",
		"appcenter-analytics": "1.12.2",
		"appcenter-crashes": "1.12.2",
		"es6-error": "^4.1.1",
		"es6-symbol": "^3.1.1",
		"i18n-js": "^3.2.1",
		"moment": "^2.24.0",
		"react": "16.8.3",
		"react-dom": "^16.7.0",
		"react-native": "^0.59.8",
		"react-native-calendars": "github:AdamGold/react-native-calendars#50f964d4a2d1aaf789568d045ecef7e1ac5762bf",
		"react-native-code-push": "^5.6.0",
		"react-native-config": "^0.11.7",
		"react-native-elements": "^1.0.0",
		"react-native-fast-image": "^5.2.0",
		"react-native-firebase": "^5.2.3",
		"react-native-gesture-handler": "^1.0.15",
		"react-native-global-props": "^1.1.5",
		"react-native-image-picker": "^0.28.1",
		"react-native-image-resizer": "^1.0.1",
		"react-native-linear-gradient": "^2.5.4",
		"react-native-material-dropdown": "^0.11.1",
		"react-native-modal": "^9.0.0",
		"react-native-modal-datetime-picker": "^6.0.0",
		"react-native-splash-screen": "^3.2.0",
		"react-native-svg": "^9.3.5",
		"react-native-vector-icons": "^6.2.0",
		"react-navigation": "^3.0.9",
		"react-redux": "^6.0.0",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0",
		"remote-redux-devtools": "^0.5.16",
		"rn-content-loader": "0.0.4",
		"rn-secure-storage": "^1.0.82",
		"validate.js": "^0.12.0"
	},
	"devDependencies": {
		"babel-preset-react-native": "^4.0.1",
		"detox": "12.1.1",
		"jest-react-native": "^18.0.0",
		"metro-react-native-babel-preset": "^0.51.1",
		"react-test-renderer": "^16.7.0",
		"redux-mock-store": "^1.5.3",
		"@babel/core": "^7.3.4",
		"@babel/runtime": "^7.3.4",
		"jest": "^24.4.0"
	},
	"jest": {
		"preset": "react-native",
		"setupFiles": [
			"./__mocks__/mockStorage.js",
			"./__mocks__/mockFetch.js",
			"./__mocks__/mockContentLoader.js",
			"./__mocks__/mockFirebase.js",
			"./__mocks__/mockFormData.js",
			"./__mocks__/mockAnalytics.js",
			"./__mocks__/mockAsyncStorage.js"
		],
		"testMatch": [
			"<rootDir>/tests/**/*.test.js"
		],
		"coverageDirectory": "./coverage/",
		"collectCoverage": true,
		"collectCoverageFrom": [
			"<rootDir>/src/**/*.{js,jsx}",
			"!<rootDir>/src/**/index.js",
			"!<rootDir>/src/App.js",
			"!<rootDir>/node_modules/",
			"!<rootDir>/src/Store.js",
			"!<rootDir>/src/reducers/index.js"
		]
	},
	"detox": {
		"configurations": {
			"ios.sim.debug": {
				"binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/dryvofront.app",
				"build": "RN_SRC_EXT=e2e.js xcodebuild -workspace ios/dryvofront.xcworkspace -scheme dryvofront -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
				"type": "ios.simulator",
				"name": "iPhone X"
			}
		},
		"test-runner": "jest"
	},
	"rnpm": {
		"assets": [
			"./assets/fonts"
		]
	}
}
